{
  "name": "ts-react-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for React projects with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/devianllert/ts-react-boilerplate.git"
  },
  "author": "Ruslan Povolockii <devianllert@gmail.com>",
  "bugs": {
    "url": "https://github.com/devianllert/ts-react-boilerplate/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "node internals/scripts/start.js",
    "build": "node internals/scripts/build.js",
    "test": "node internals/scripts/test.js",
    "lint": "npm run lint:eslint && npm run lint:css",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:css:fix",
    "lint:eslint": "eslint --ext .tsx,.ts .",
    "lint:eslint:fix": "eslint --fix --ext .tsx,.ts .",
    "lint:css": "stylelint --fix **/*.scss",
    "lint:css:fix": "stylelint **/*.scss"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "^5.1.0",
    "history": "^4.10.1",
    "i18next": "^19.0.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "popper.js": "^1.16.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.0.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@svgr/webpack": "4.3.3",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.3",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.5",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-helmet": "^5.0.14",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-transition-group": "^4.2.3",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "^25.0.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-preset-react-app": "^9.0.2",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "2.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "file-loader": "4.2.0",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": "^3.0.9",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^2.1.1",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.3.0",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react-app-polyfill": "^1.0.4",
    "react-dev-utils": "^9.1.0",
    "react-test-renderer": "^16.11.0",
    "resolve": "1.11.0",
    "sass-loader": "7.1.0",
    "semver": "6.2.0",
    "style-loader": "0.23.1",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-scss": "^3.12.1",
    "terser-webpack-plugin": "1.3.0",
    "ts-pnp": "1.1.4",
    "typescript": "3.7.2",
    "url-loader": "2.1.0",
    "webpack": "4.41.0",
    "webpack-dev-server": "3.8.2",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:eslint:fix",
      "git add"
    ],
    "*.scss": [
      "npm run lint:css:fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
