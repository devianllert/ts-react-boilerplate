{
  "name": "ts-react-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for React projects with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/devianllert/ts-react-boilerplate.git"
  },
  "author": "Ruslan Povolockii <devianllert@gmail.com>",
  "homepage": "https://github.com/devianllert/ts-react-boilerplate",
  "bugs": {
    "url": "https://github.com/devianllert/ts-react-boilerplate/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "npm run lint:eslint && npm run lint:css",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:css:fix",
    "lint:eslint": "eslint --ext .tsx,.ts .",
    "lint:eslint:fix": "eslint --fix --ext .tsx,.ts .",
    "lint:css": "stylelint --fix **/*.scss",
    "lint:css:fix": "stylelint **/*.scss"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dotenv": "8.0.0",
    "history": "^4.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@svgr/webpack": "4.3.2",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.7",
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "2.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.5.0",
    "file-loader": "4.1.0",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": "^3.0.0",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^2.1.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "resolve": "1.11.0",
    "sass-loader": "7.1.0",
    "semver": "6.2.0",
    "style-loader": "0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.2",
    "terser-webpack-plugin": "1.3.0",
    "ts-pnp": "1.1.2",
    "typescript": "3.5.3",
    "url-loader": "2.1.0",
    "webpack": "4.36.0",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:eslint:fix",
      "git add ."
    ],
    "*.scss": [
      "npm run lint:css:fix",
      "git add ."
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
