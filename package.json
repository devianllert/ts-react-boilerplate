{
  "name": "ts-react-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for React projects with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/devianllert/ts-react-boilerplate.git"
  },
  "author": "Ruslan Povolockii <devianllert@gmail.com>",
  "bugs": {
    "url": "https://github.com/devianllert/ts-react-boilerplate/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm run lint:eslint && npm run lint:css",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:css:fix",
    "lint:eslint": "eslint --ext .tsx,.ts .",
    "lint:eslint:fix": "eslint --fix --ext .tsx,.ts .",
    "lint:css": "stylelint **/*.{ts,tsx}",
    "lint:css:fix": "stylelint --fix **/*.{ts,tsx}",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "connected-react-router": "^6.8.0",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "i18next": "^19.4.0",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-xhr-backend": "^3.2.2",
    "query-string": "^6.12.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-essential-tools": "^0.37.3",
    "react-helmet": "^6.0.0",
    "react-i18next": "^11.3.4",
    "react-icons": "^3.9.0",
    "react-query": "^1.2.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.0",
    "styled-theming": "^2.2.0",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/react-query": "^1.1.1",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-transition-group": "^4.2.4",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.0.1",
    "@types/styled-theming": "^2.2.2",
    "@types/yup": "^0.28.1",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-testing-library": "^3.1.0",
    "husky": "^4.2.5",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "^10.1.3",
    "react-scripts": "3.4.1",
    "source-map-explorer": "^2.4.2",
    "stylelint": "^13.3.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
